.topbar-outer.dark
  .topbar.content-width
    .table.fullheight
      .table-cell.fullheight.middle
        .logo
          %a{href: "index.html"}
            %img{alt: "", height: "17", src: asset_path("logos/logo_white.png")}/
    %ul.topsocial
      %li
        %a{href: "http://www.linkedin.com/pub/graham-jenson/6/a93/b07"}
          %i.fa.fa-linkedin-square
          .tooltip LinkedIn
      %li
        %a{href: "https://twitter.com/grahamjenson"}
          %i.fa.fa-twitter
          .tooltip Twitter
      %li
        %a{href: "https://github.com/grahamjenson"}
          %i.fa.fa-github
          .tooltip Github
    %ul.topnav
      - tab = yield(:tab)
      %li
        %a{href: "index.html", class: (tab == 'home' ? 'current' : '')} Home
      %li
        %a{href: "about.html", class: (tab == 'about' ? 'current' : '')} About Us
      %li
        %a{href: "portfolio.html", class: (tab == 'portfolio' ? 'current' : '')} Portfolio
      %li
        %a{href: "blog.html", class: (tab == 'blog' ? 'current' : '')} Blog
      %li
        %a{href: "contact.html", class: (tab == 'contact' ? 'current' : '')} Contact Us
    %a.mobilenav-click{href: "#"}
      .mobilenav-button-container
        .mobilenav-button-inner
          .mobilenav-button
    .mobilenav-container
      %ul.mobilenav